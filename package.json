{
  "name": "hero-app-server",
  "version": "1.0.0",
  "description": "Backend for hero-app",
  "main": "dist/server.js",
  "scripts": {
    "clear-cache": "jest --clearCache",
    "clean": "rm -rf dist || true && rm -rf coverage || true",
    "pretest": "npm run clear-cache",
    "test": "DATABASE=graphql DATABASE_USER=graphql DATABASE_PWD=graphql jest",
    "lint": "tslint -c tslint.json '{src,test}/**/*.{ts,tsx}' --fix",
    "prebuild": "npm run lint",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "dev": "nodemon --watch src -e ts --exec npm start",
    "predocker": "docker pull postgres",
    "docker": "docker run -d --name app-server -p 5432:5432 -e 'POSTGRES_PASSWORD=graphql' postgres",
    "docker:stop": "docker stop app-server",
    "docker:start": "docker start app-server"
  },
  "keywords": [
    "NodeJs",
    "TS",
    "Appollo Server",
    "Graph QL",
    "Sequelize"
  ],
  "author": "microtema@web.de",
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.14",
    "@types/node": "^10.14.9",
    "@types/validator": "^10.11.1",
    "apollo-server-express": "^2.6.2",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "faker": "^4.1.0",
    "graphql": "^14.3.1",
    "graphql-tools": "^4.0.4",
    "jest": "^24.8.0",
    "lodash": "^4.17.19",
    "model-builder-ts": "^2.0.0",
    "nodemon": "^1.19.1",
    "qs-middleware": "^1.0.3",
    "recursive-readdir-synchronous": "0.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1",
    "typescript-ioc": "^1.2.5"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "azure-jwt-verify": "^1.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "pg": "^7.11.0",
    "sequelize": "^5.8.9"
  }
}
